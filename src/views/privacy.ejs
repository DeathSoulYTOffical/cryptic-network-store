<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Privacy Policy - <%= sitesettings.sitename %></title>
        <meta property="og:title" content="Privacy Policy - <%= sitesettings.sitename %>" />
        <link rel="icon" href="/assets/logo.png">
        <meta property="og:image" content="/assets/logo.png">
        <meta name="theme-color" content="<%= sitesettings.sitecolor %>">
        <meta name="description" content="<%= sitesettings.sitedesc %>" />
        <meta property="og:type" content="website" />
        <meta property="og:description" content="<%= sitesettings.sitedesc %>" />
    </head>
    <body>
        <%- include('_header') %>
        <h1><%= sitesettings.sitename %>'s Privacy Policy</h1>
        <hr>
        <br>
        <div class="container text-start">
            <%- converted %>
        </div>
        <%- include('_footer') %>
    </body>
</html>
